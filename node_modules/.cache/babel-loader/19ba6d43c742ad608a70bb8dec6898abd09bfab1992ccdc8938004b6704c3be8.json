mitForm(){\r\n            axios.post('http://laravelpractice.local/api/register', {'email': this.email, 'name': this.name, 'password': this.password})\r\n                .then((response) => { alert(JSON.stringify(response.data)) })\r\n                 .catch((error) => {\r\n                    this.emailError = (JSON.stringify(error.response.data.message.email).slice(2,-2)) || '';\r\n                    this.nameError = (JSON.stringify(error.response.data.message.name).slice(2,-2)) || '';\r\n                    this.passwordError = (JSON.stringify(error.response.data.message.password).slice(2,-2)) || '' ;\r\n                 });\r\n        }\r\n\r\n        \r\n    },\r\n}\r\n\r\n\r\n</script>\r\n\r\n<style scoped>\r\n\r\n.register{\r\n\r\n    padding: 20%;\r\n    background-color: rgba(165, 42, 42, 0.186);\r\n \r\n    text-align: center;\r\n    \r\n}\r\n.errorMessage{\r\n    color:red;\r\n}\r\n\r\ninput{\r\n\r\n    margin-bottom:2rem;\r\n}\r\n</style>"],"mappings":"AA2BA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAc;EACVC,IAAI,EAAE,mBAAmB;EAEzBC,IAAIA,CAAA,EAAE;IACF,OAAM;MACFD,IAAI,EAAC,EAAE;MACPE,KAAK,EAAC,EAAE;MACRC,QAAQ,EAAC,EAAE;MACXC,cAAc,EAAC,EAAE;MACjBC,YAAY,EAAC,EAAE;MAEfC,UAAS,EAAG,EAAE;MACdC,SAAQ,EAAG,EAAE;MACbC,aAAY,EAAG,EAAE;MACjBC,iBAAiB,EAAE;IACvB,CAAC;EAEL,CAAC;EACDC,OAAOA,CAAA,EAAE,CAET,CAAC;EACDC,OAAO,EAAE;IACLC,cAAcA,CAAA,EAAE;MACZ,IAAG,IAAI,CAACZ,IAAG,KAAM,EAAC,IAAK,IAAI,CAACE,KAAI,KAAM,EAAC,IAAK,IAAI,CAACE,cAAa,KAAM,EAAC,IAAK,IAAI,CAACD,QAAO,KAAM,EAAC,EAAG;QAC5F,IAAG,IAAI,CAACC,cAAa,IAAK,IAAI,CAACD,QAAO,EAAG;UAExC,IAAI,CAACE,YAAW,GAAI,EAAE;UACtB,IAAI,CAACI,iBAAgB,GAAI,KAAK;QAE9B,CAAC,MAAI;UACF,IAAI,CAACJ,YAAW,GAAI,uBAAuB;QAC/C;MAEJ,CAAC,MAAI;QACD,IAAI,CAACI,iBAAgB,GAAI,IAAI;MACjC;IACJ,CAAC;IACFI,UAAUA,CAAA,EAAE;MACPd,KAAK,CAACe,IAAI,CAAC,2CAA2C,EAAE;QAAC,OAAO,EAAE,IAAI,CAACZ,KAAK;QAAE,MAAM,EAAE,IAAI,CAACF,IAAI;QAAE,UAAU,EAAE,IAAI,CAACG;MAAQ,CAAC,EACtHY,IAAI,CAAEC,QAAQ,IAAK;QAAEC,KAAK,CAACC,IAAI,CAACC,SAAS,CAACH,QAAQ,CAACf,IAAI,CAAC;MAAE,CAAC,EAC1DmB,KAAK,CAAEC